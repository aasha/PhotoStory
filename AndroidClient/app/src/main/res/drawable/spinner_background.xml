<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape >
            <solid
                android:color="@color/white"/>
            <corners
                android:radius="10dp" />
            <stroke
                android:width="1dp"
                android:color="@color/app_blue" >
            </stroke>
        </shape>
    </item>
    <item android:height="20dp"
        android:drawable="@drawable/drop_down_arrow"
        android:gravity="end"
        android:left="250dp"
        android:top="10dp"
        android:bottom="10dp"
        android:right="10dp"
        android:width="20dp">

    </item>
</layer-list>
